{
  "name": "node-bullmq-jobs-fixture",
  "version": "1.0.0",
  "description": "BullMQ job queue fixture for testing queue/worker extraction, job scheduling, retries, and error handling",
  "scripts": {
    "worker:email": "node workers/email-worker.js",
    "worker:image": "node workers/image-worker.js",
    "worker:report": "node workers/report-worker.js",
    "workers:all": "concurrently \"npm run worker:email\" \"npm run worker:image\" \"npm run worker:report\""
  },
  "dependencies": {
    "bullmq": "5.63.2",
    "ioredis": "5.8.2",
    "nodemailer": "7.0.10",
    "sharp": "0.34.5"
  },
  "devDependencies": {
    "concurrently": "9.2.1"
  }
}
